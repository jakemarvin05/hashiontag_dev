<!doctype html>
<!--[if lt IE 7 ]><html lang="en" class="no-js ie ie6"><![endif]-->
<!--[if IE 7 ]><html lang="en" class="no-js ie ie7"><![endif]-->
<!--[if IE 8 ]><html lang="en" class="no-js ie ie8"><![endif]-->
<!--[if IE 9 ]><html lang="en" class="no-js ie ie9"><![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--><html lang="en" class="no-js" ><!--<![endif]-->
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>{title.title}</title>

    <meta name="description" content="{title.desc}">
    <meta name="viewport" content="initial-scale=1.0, user-scalable=no">

    <!-- START Social media metas -->
    <meta property="og:title" content="{title.title}">
    <meta property="og:description" content="{title.desc}">
    <meta property="og:type" content="website">
    <meta property="og:image" content="http://vogueverve.com/{title.img}">

    <!-- END Social media metas -->

    <!-- CSS -->
    <link href="{p.css}/style.css" rel="stylesheet"> 

    <!-- fonts, glyphicons -->
    <link href="{p.css}/bootstrap-glyphicons.css" rel="stylesheet">
    <style>
    /* glyphicon overwrite */
    .glyphicon { top: 2px; padding: 0px 3px;}
    button .glyphicon { top: 0px; padding: 0px;}
    </style>

    <!-- global object for all uses where necessary -->
    <script>var VVGLOBAL = {}</script>
    
    <!-- jQuery and UI -->
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>

    <!-- fancyBox image gallery -->
    <link rel="stylesheet" href="{p.js}/fancybox/source/jquery.fancybox.css?v=2.1.5" type="text/css" media="screen" />

    <!-- placeholder polyfill css (js is in footer) -->
    <link rel="stylesheet" href="{p.js}/placeholderpolyfill/placeholder_polyfill.min.css">

    <!-- Tags Highlight -->
    <link href="{p.js}/jtags/jquery.hashtags.css" rel="stylesheet"> 
    <script>
    if (navigator.userAgent.indexOf('iPhone') !== -1) {
      document.write('<link rel="stylesheet" href="{p.js}/jtags/jquery.hashtags.iphone.css">');
    }
    </script>


    <script src="{p.js}/vvUtils.js"></script>
    
    <!-- preloads -->
    <script>
    var preload1 = new Image(); 
    preload1.src ="{f.imgLoaderHolder}";
    </script>

{?printHead}
<script>
var printHead = {printHead|s};
var P = printHead.p;
</script>
{/printHead}

{?renderJSON}
<script>
var renderJSON = {renderJSON|s};
</script>
{/renderJSON}