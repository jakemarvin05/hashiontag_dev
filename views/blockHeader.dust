<!doctype html>
<!--[if lt IE 7 ]><html lang="en" class="no-js ie ie6"><![endif]-->
<!--[if IE 7 ]><html lang="en" class="no-js ie ie7"><![endif]-->
<!--[if IE 8 ]><html lang="en" class="no-js ie ie8"><![endif]-->
<!--[if IE 9 ]><html lang="en" class="no-js ie ie9"><![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--><html lang="en" class="no-js" ><!--<![endif]-->
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>{title.title}</title>

    <meta name="description" content="{title.desc}">
    <meta name="viewport" content="initial-scale=1.0, user-scalable=no">

    <!--<meta name="google-site-verification" content="uIeAf16Sh0CavT6Rg1VtFAmlE8RlT4qrk0OhOvNRSHU" />-->

    <!-- START Social media metas -->

    <!-- Google Authorship and Publisher Markup 
    <link rel="author" href="https://plus.google.com/[Google+_Profile]/posts">
    <link rel="publisher" href="https://plus.google.com/[Google+_Page_Profile]">
    -->

    <meta property="og:title" content="{title.title}">
    <meta property="og:description" content="{title.desc}">
    <meta property="og:type" content="website">
    <meta property="og:image" content="http://vogue-verve.com/{title.img}">


    <!-- Twitter Card data
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:site" content="@publisher_handle">
    <meta name="twitter:title" content="Page Title">
    <meta name="twitter:description" content="Page description less than 200 characters">
    <meta name="twitter:creator" content="@author_handle">
    -->
    <!-- Twitter summary card with large image must be at least 280x150px 
    <meta name="twitter:image:src" content="http://www.example.com/image.html">
    -->

    <!-- END Social media metas -->

    <!-- CSS -->
    <link href="{p.css}/style.css" rel="stylesheet"> 

    <!-- fonts, glyphicons -->
    <link href="{p.css}/bootstrap-glyphicons.css" rel="stylesheet">
    <style>
    /* glyphicon overwrite */
    .glyphicon { top: 2px; padding: 0px 3px;}
    button .glyphicon { top: 0px; padding: 0px;}
    </style>

    <!-- global object for all uses where necessary -->
    <script>var VVGLOBAL = {}</script>
    
    <!-- jQuery and UI -->
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <!--<script src="{p.js}/jquery-1.10.1.min.js"></script>-->

    <!-- fancyBox image gallery -->
    <link rel="stylesheet" href="{p.js}/fancybox/source/jquery.fancybox.css?v=2.1.5" type="text/css" media="screen" />

    <!-- placeholder polyfill css (js is in footer) -->
    <link rel="stylesheet" href="{p.js}/placeholderpolyfill/placeholder_polyfill.min.css">

    <script src="{p.js}/vvUtils.js"></script>
    
    <!-- preloads -->
    <script>
    var preload1 = new Image(); 
    preload1.src ="{f.imgLoaderHolder}";
    </script>

{?printHead}
<script>
var printHead = {printHead|s};
</script>
{/printHead}

{?renderJSON}
<script>
var renderJSON = {renderJSON|s};
</script>
{/renderJSON}